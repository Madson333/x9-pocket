{
  "expo": {
    "name": "x9-pocket",
    "slug": "x9-pocket",
    "version": "1.0.0",
    "android": {
      "config": {
        "googleMaps": {
          "apiKey": "YOUR_GOOGLE_MAPS_API_KEY"
        }
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "ios": {
      "infoPlist": {
        "NSCameraUsageDescription": "Este aplicativo precisa acessar a câmera para permitir denúncias com foto.",
        "NSLocationWhenInUseUsageDescription": "Precisamos da sua localização para marcar onde a denúncia ocorreu."
      },
      "supportsTablet": true
    },
    "orientation": "portrait",
    "scheme": "x9pocket",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    }
  }
}
